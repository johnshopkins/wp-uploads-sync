{
  "name": "johnshopkins/wp-uploads-sync",
  "type": "wordpress-plugin",
  "description": "On attachment upload, delete, edit, run a script to rsync uploads between servers.",
  "license": "MIT",
  "homepage": "http://github.com/johnshopkins/wp-uploads-sync",
  "require": {
    "johnshopkins/akamai-open-edgegrid-php": "v1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "php-mock/php-mock-phpunit": "^1.1"
  },
  "autoload": {
    "psr-4" : {
      "UploadsSync\\" : ["src/", "tests/"]
    }
  }
}
